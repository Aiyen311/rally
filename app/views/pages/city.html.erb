<div class = "container-fluid city_form">
	<div class = "row">
		<div class = "col-md-5 offset-md-3">
			<div class = "text-center form-group">
				<%= form_for @city do |f| %>
				<%= f.label "Enter City: " %>
				<%= f.text_field :city %>
				<%= f.submit %>
				<% end %>
			</div>
		</div>
	</div>
</div>